{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Server=<host>;Port=<port>;User Id=<user>;Password=<password>;Database=<database>"
  },
  "Encryption": {
    "Key": "<32-char key>",
    "IV": "<16-char key>"
  },
  "Frontend": {
    "Address": "<address>"
  },
  "MailSettings": {
    "FromMail": "<fromemail>",
    "DisplayName": "<displayname>",
    "Password": "<password>",
    "SmtpHost": "<host>",
    "SmtpPort": 0
  },
  "JwtSettings": {
    "Key": "<key>",
    "Issuer": "<issuer>",
    "Audience": "<audience>",
    "AccessTokenValidMinutes": 0,
    "RefreshTokenValidMinutes": 0
  }
}
